{
  "apiTestingConfig": {
    "projectName": "API Testing with Postman",
    "version": "1.0.0",
    "description": "Comprehensive API testing suite for authentication and data retrieval",
    "environment": "production",
    "baseURL": "https://api.example.com/v1"
  },
  "testSettings": {
    "timeout": 5000,
    "retries": 2,
    "verbose": true,
    "reportFormat": "html"
  },
  "authentication": {
    "type": "Bearer",
    "tokenExpiry": 3600,
    "refreshToken": true
  },
  "endpoints": {
    "auth": "/auth",
    "users": "/users",
    "profile": "/profile",
    "data": "/data"
  },
  "testCases": {
    "authentication": 2,
    "dataRetrieval": 5,
    "errorHandling": 1,
    "total": 8
  },
  "expectations": {
    "successRate": 95,
    "averageResponseTime": 500,
    "maxResponseTime": 3000
  },
  "logging": {
    "enabled": true,
    "level": "info",
    "output": "console,file"
  }
}
